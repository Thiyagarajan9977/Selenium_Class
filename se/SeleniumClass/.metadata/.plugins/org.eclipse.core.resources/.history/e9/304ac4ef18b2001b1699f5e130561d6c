package org.list;

import java.util.*;

public class Collections {

	// Storing Multiple Value of dismilar data types in a single reference name,
	// Memory wastage is low due to memory is allocated at run time.

	public static void main(String[] args) {

		List li = new ArrayList();

		li.add(10);
		li.add("Thiyagu");
		li.add(25.23);
		li.add(123456789);
		li.add('m');
		li.add(true);
		li.add(10);
		System.out.println(li);

		// size of list

		int size = li.size();
		System.out.println(size);

	}

}
